{
  "expo": {
    "name": "メモリス",
    "slug": "SvgApp",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#c3e8fe"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_LOAD"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.hiroapp.handwritten",
      "buildNumber": "1.0.4",
      "infoPlist": {
        "CFBundleDevelopmentRegion":"ja_JP",
        "NSPhotoLibraryUsageDescription": "メモ画像を保存するためにフォトライブラリを使用します",
        "NSPhotoLibraryAddUsageDescription": "メモ画像を保存するためにフォトライブラリを使用します"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.hiroapp.handwritten"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
