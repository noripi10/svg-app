{
  "expo": {
    "name": "メモリス",
    "slug": "SvgApp",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#c3e8fe"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_LOAD"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.hiroapp.handwritten",
      "buildNumber": "1.1.2",
      "infoPlist": {
        "CFBundleDevelopmentRegion": "ja_JP",
        "NSPhotoLibraryUsageDescription": "メモ画像を保存するためにフォトライブラリを使用します",
        "NSPhotoLibraryAddUsageDescription": "メモ画像を保存するためにフォトライブラリを使用します",
        "NSUserTrackingUsageDescription": "広告のパーソナライズのみを目的として使用され、表示される広告が増えることはありません"
      },
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-7379270123809470~4236128830"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.hiroapp.handwritten",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-7379270123809470~4236128830"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
